  font-smoothing: antialiased
  -webkit-font-smoothing: antialiased
  -moz-osx-font-smoothing: grayscale

.page-header
  min-height: 376px
  background: #1F2633
  color: $color_white
  padding: 0 152px 0 152px
  background: url('images/header-bg.png') center no-repeat
  background-size: cover
  overflow: hidden
  +respond-to-retina
    background: url('images/header-bg@2x.png') center no-repeat
    background-size: cover

  &--home
    min-height: 600px

  &--contact
    min-height: 346px

.page-header__toggle-btn
  display: none
  position: relative
  width: 24px
  height: 24px
  &:before
    content: ''
    position: absolute
    top: 6px
    left: 5px
    width: 14px
    height: 2px
    background: rgba(255, 255, 255, 1)
    box-shadow: 0 5px 0 0 rgba(255, 255, 255, 1), 0 10px 0 0 rgba(255, 255, 255, 1)
    cursor: pointer

.page-header__close-btn
  display: none
  position: relative
  width: 24px
  height: 24px
  &:before
    content: ''
    position: absolute
    top: 6px
    left: 6px
    width: 12px
    height: 12px
    background: url('images/close-icon.svg')
    cursor: pointer

.page-header__logo
  padding-top: 16px
  margin-right: 100px
  img
    width: 144px
    height: 16px

.main-nav__item
  margin-right: 24px
  padding-top: 24px
  &:last-child
    margin-right: 0

.main-nav__link
  position: relative
  font-size: 16px
  line-height: 16px
  font-weight: 400
  color: #9198ad
  padding-top: 29px
  transition: .25s ease

.main-nav__link--active
  color: #ffffff
  font-weight: 500
  margin-left: 16px
  &:before
    content: ''
    height: 7px
    width: 7px
    position: absolute
    left: -15px
    top: 35px
    background: url('images/oval-white.svg')

  &:after
    content: ''
    height: 30px
    width: 1px
    position: absolute
    left: -12px
    top: 0
    background: #ffffff

.page-header__top
  display: flex
  flex-direction: row
  justify-content: space-between
  align-items: center
  margin-bottom: 88px

.page-header__callback
  display: flex
  flex-direction: row
  align-items: flex-end
  padding-top: 16px

.page-header__callback-toggle
  display: none
  position: relative
  width: 24px
  height: 24px
  &:before
    content: ''
    position: absolute
    top: 4px
    left: 5px
    width: 14px
    height: 14px
    background: url('images/phone-icon.svg') center no-repeat
    background-size: cover
    cursor: pointer

.page-header__callback-close
  display: none
  position: relative
  width: 24px
  height: 24px
  &:before
    content: ''
    position: absolute
    top: 6px
    left: 6px
    width: 12px
    height: 12px
    background: url('images/close-icon.svg') center no-repeat
    background-size: cover
