  font-size: 10px
  line-height: 16px
  font-weight: 700
  color: #50cfe3
  text-transform: uppercase

.page-header__callback-lower
  display: block
  font-size: 16px
  line-height: 16px
  font-weight: 700
  color: #ffffff

.page-header__callback-button
  font-size: 10px
  line-height: 16px
  font-weight: 700
  text-transform: uppercase
